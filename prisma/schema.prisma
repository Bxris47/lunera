datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Appointment {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  date      DateTime
  time      String
  status    String   @default("pending")
  createdAt DateTime @default(now())
}
